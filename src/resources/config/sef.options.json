{
  "thresholds":{
    "checks":[
      {
        "threshold":"rate == 1",
        "abortOnFail":true
      }
    ],
    "App Onboard Duration":[
      "p(99)<900000",
      "avg<900000",
      "med<900000"
    ],
    "Enable App Duration":[
      "p(99)<1500",
      "avg<1500",
      "med<1500"
    ],
    "Instantiating Duration":[
      "p(99)<300000",
      "avg<300000",
      "med<300000"
    ],
    "Terminating Duration":[
      "p(99)<180000",
      "avg<180000",
      "med<180000"
    ],
    "Delete App Instance Duration":[
      "p(99)<35000",
      "avg<35000",
      "med<35000"
    ],
    "Disable App Duration":[
      "p(99)<1500",
      "avg<1500",
      "med<1500"
    ],
    "Delete App Without Instance Duration":[
      "p(99)<5000",
      "avg<5000",
      "med<5000"
    ],
    "http_req_failed":[
      "rate<=0.01"
    ]
  },
